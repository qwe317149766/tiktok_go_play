SUCCESS_COUNT=1                   # SUCCESS_COUNT: 当前已成功注册的账号数（从 backup 加载），默认 3
ENABLE_IOS_UA=true                 # ENABLE_IOS_UA: 是否启用 iOS User-Agent，false 为关闭
CONCURRENCY=200                     # CONCURRENCY: 并发工作线程数上限
FINALIZE_RETRIES=30                 # FINALIZE_RETRIES: finalize 步骤最大重试次数
MAX_SUCCESS_PER_FILE=10             # MAX_SUCCESS_PER_FILE: 每个成功文件（<date>-注册成功-<n>.txt）最大写入行数
TOTAL_SUCCESS_LIMIT=10000           # TOTAL_SUCCESS_LIMIT: 所有成功文件累计最大行数，达到后停止注册
SMS_POLL_TIMEOUT_SEC=60             # SMS_POLL_TIMEOUT_SEC: 等待短信验证码的超时时间（秒）
HTTP_REQUEST_TIMEOUT_SEC=15         # HTTP_REQUEST_TIMEOUT_SEC: 所有 HTTP 请求的超时时间（秒）
PollTimeoutSec=300                  # PollTimeoutSec: 全局轮询（PollUntilParamSuccess）超时时间（秒）
ENABLE_ANOMALOUS_UA=false          # ENABLE_ANOMALOUS_UA: 是否启用异常 UA (Model/Device/Hardware 随机 6 位字母)